package JavaBasics;

public class LoopConcept {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//while loop
		
		int i = 0; // initialization
		while(i<=10)// Condition
		{
			System.out.println(i);
			i++;// Increment
		}
		
		//for loop
		for(int j = 0; j<=10;j++)
		{
			System.out.println(j);
		}
		
		
		
	}

}
